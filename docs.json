{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "StableNet Developer Docs",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/images/WEMIXFI_Monochrome_Black.png",

  "navigation": {
    "global": {
      "languages": [
        {
          "language": "ko",
          "href": "https://wemix.fi"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Testnet Docs",
        "groups": [
          {
            "group": "StableNet",
            "pages": [
              "testnet-docs/stable-net/test",
              "testnet-docs/stable-net/overview"
            ]
          },
          {
            "group": "Getting Started",
            "pages": [
              "testnet-docs/getting-started/installation-and-building",
              "testnet-docs/getting-started/node-configuration",
              "testnet-docs/getting-started/genesis-setup-and-network-types"
            ]
          }
        ]
      }
    ]
  },

  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "StableNet",
        "href": "https://stablenet.network"
      },
      {
        "label": "Explorer",
        "href": "https://explorer.stablenet.network"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
